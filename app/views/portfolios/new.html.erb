<h1>Create a new Portfolio Item</h1>

<%= form_for(@portfolio_item) do |f| %>
<div class="field">
  <%= f.label :title %>
  <%= f.text_field :title, id: :blog_title %>
</div>

<div class="field">
  <%= f.label :body %>
  <%= f.text_area :body, id: :blog_body %>
</div>

<div class="actions">
  <%= f.submit %>
</div>
<% end %>
